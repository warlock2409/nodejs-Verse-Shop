<form action="/cart" method="post">
    <% if (product.Qty > 0) { %>
    <button class="btn" type="submit"> <i class="fa-solid fa-cart-plus"></i> Add to Cart </button>
    <input type="hidden" name="productId" value="<%= product.id %>">
    <%} else {%>
        <h5 style="color: red; display: inline;" class="btn"><i class="fa-solid fa-business-time"></i> Out of Stock </h5>
        <%}%>
</form> 